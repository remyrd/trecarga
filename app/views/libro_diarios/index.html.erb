<h1>Listing libro_diarios</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Tipo cambio</th>
      <th>Cuenta</th>
      <th>Deber</th>
      <th>Haber</th>
      <th>Glosario</th>
      <th>Obsoleto</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @libro_diarios.each do |libro_diario| %>
      <tr>
        <td><%= libro_diario.fecha %></td>
        <td><%= libro_diario.tipo_cambio %></td>
        <td><%= libro_diario.cuenta %></td>
        <td><%= libro_diario.deber %></td>
        <td><%= libro_diario.haber %></td>
        <td><%= libro_diario.glosario %></td>
        <td><%= libro_diario.obsoleto %></td>
        <td><%= libro_diario.tipo %></td>
        <td><%= link_to 'Show', libro_diario %></td>
        <td><%= link_to 'Edit', edit_libro_diario_path(libro_diario) %></td>
        <td><%= link_to 'Destroy', libro_diario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Libro diario', new_libro_diario_path %>
